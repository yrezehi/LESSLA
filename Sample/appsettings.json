{
  "Lessla": {
    "Serilog": {
      "Using": [ "Serilog.Sinks.Http", "Serilog.Exceptions" ],
      "MinimumLevel": "Error",
      "WriteTo": [
        {
          "Name": "DurableHttpUsingFileSizeRolledBuffers",
          "Args": {
            "requestUri": "http://*******"
          }
        }
      ],
      "Enrich": [ "FromLogContext", "WithExceptionDetails" ],
      "Properties": {
        "Application": "YOU_APPLICATION_NAME"
      }
    },
    "HealthCheck": {
      "Application": "YOU_APPLICATION_NAME",
      "ExternalEndpoints": [

      ]
    }
  },
  "AllowedHosts": "*"
}
