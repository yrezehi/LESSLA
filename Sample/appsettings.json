{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Http", "Serilog.Exceptions" ],
    "MinimumLevel": "Error",
    "WriteTo": [
      {
        "Name": "DurableHttpUsingFileSizeRolledBuffers",
        "Args": {
          "requestUri": "http://*******"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithExceptionDetails" ],
    "Properties": {
      "Application": "Sample"
    }
  },
  "Lessla": {
    "HealthCheck": {
      "ExternalEndpoints": [

      ]
    }
  },
  "AllowedHosts": "*"
}
